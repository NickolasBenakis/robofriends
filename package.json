      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "fetch-mock": "^7.5.1",
    "gh-pages": "^2.1.1",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "react-redux": "^7.1.1",
    "react-test-renderer": "^16.11.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "tachyons": "^4.11.1"
  }
